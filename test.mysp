class demo inherits d {

    public    dynamic a;
    private   dynamic b;
    protected dynamic c;
    private   static  d;
    
    public dynamic demo() {
        this->a = 1;
        this->b = 2;
        this->c = 3;
        demo::d = 4;
    }
    
    public dynamic foo(n = 1, j = 2) {
        for (i = 0; i < j; i++) {
            print n * ( a + b + c );
            this->blah();
        }
    }
    
    public static bar(n) {
        print n * d;
    }

}


