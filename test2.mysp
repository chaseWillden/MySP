class Person {

    protected dynamic name;
    protected dynamic age;
    protected dynamic gender;
    
    public dynamic getAge() {
        return this->age;
    }
    
    public dynamic getGender() {
        return (this->gender == 'f') ? 'female' : 'male';
    }
    
    public dynamic getName() {
        return this->name;
    }

}

class Nichole inherits Person {
    
    public dynamic Nichole() {
        print "Hello\n";
        this->name = "Nichole";
        this->age = 24;
        this->gender = 'f';
    }

}

main {
    n = Nichole();
    print n->getName() . " is " . n->getAge() . "!\n";
    print n->getName() . " is " . n->getGender() . "!\n";
    print (1 == 1) ? 1 : 2;
    print ((1 == 1) ? 1 : 2) . 'Yes';
}